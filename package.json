    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-relay": "1.8.3",
    "eslint-plugin-typescript": "0.14.0",
    "git-json-merge": "^0.4.4",
    "https-browserify": "^1.0.0",
    "hygen": "6.2.11",
    "jest": "29.3.1",
    "jest-environment-jsdom": "^29.0.1",
    "jest-junit": "15.0.0",
    "lerna": "^6.0.1",
    "lint-staged": "^13.0.0",
    "os-browserify": "^0.3.0",
    "patch-package": "^6.5.0",
    "path-browserify": "^1.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "2.8.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typescript": "4.9.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.yml": [
      "prettier --write"
    ]
  },
  "pre-commit": "lint:staged",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/sibelius/relay-workshop.git"
  },
  "resolutions": {
    "graphql": "16.6.0",
    "html-webpack-plugin": "5.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-system": "5.1.5",
    "webpack": "5.74.0"
  },
  "scripts": {
    "b": "babel-node --extensions \".es6,.js,.es,.jsx,.mjs,.ts,.tsx\"",
    "build:server": "yarn workspace @workshop/server build",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "copy-envs": "cp apps/server/.env.example apps/server/.env && cp apps/web/.env.example apps/web/.env",
    "es": "node -r esbuild-register",
    "get-token": "yarn b apps/server/scripts/getToken.ts",
    "jest": "jest",
    "lint": "yarn eslint --fix",
    "lint:staged": "lint-staged",
    "postinstall": "yarn patch-package",
    "prettier": "prettier --write **/src/**/*.ts && prettier --write **/src/**/*.tsx",
    "relay": "lerna run relay --parallel --ignore @workshop/0* --ignore @workshop/1*",
    "seed": "yarn workspace @workshop/server seed",
    "server": "yarn workspace @workshop/server start",
    "server:serve": "yarn workspace @workshop/server serve",
    "start": "concurrently --kill-others \"yarn workspace @workshop/web start\" \"yarn workspace @workshop/server start\"",
    "update-schema": "yarn w apps/server/scripts/updateSchema.ts",
    "schema": "yarn w apps/server/scripts/updateSchema.ts",
    "w": "yarn b webpackx.ts",
    "web": "yarn workspace @workshop/web start",
    "next:dev": "yarn workspace @workshop/next dev",
    "next:build": "yarn workspace @workshop/next build",
    "nextapp:dev": "yarn workspace @workshop/nextapp dev",
    "nextapp:build": "yarn workspace @workshop/nextapp build"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*",
      "workshop/*",
      "solutions/*"
    ]
  }
}
