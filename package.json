		"graphql": "16.8.1",
		"html-webpack-plugin": "5.6.0",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-router-dom": "6.26.0",
		"styled-system": "5.1.5"
	},
	"lint-staged": {
		"*.{js,ts,tsx}": ["prettier --write", "eslint --fix"],
		"*.yml": ["prettier --write"]
	},
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/sibelius/relay-workshop.git"
	},
	"scripts": {
		"b": "babel-node --extensions \".es6,.js,.es,.jsx,.mjs,.ts,.tsx\"",
		"build:server": "pnpm workspace @workshop/server build",
		"commitmsg": "commitlint -e $GIT_PARAMS",
		"copy-envs": "cp apps/server/.env.example apps/server/.env && cp apps/web/.env.example apps/web/.env",
		"es": "node -r esbuild-register",
		"get-token": "pnpm b apps/server/scripts/getToken.ts",
		"jest": "jest",
		"lint": "pnpm eslint --fix",
		"lint:staged": "lint-staged",
		"postinstall": "pnpm patch-package",
		"prettier": "prettier --write **/src/**/*.ts && prettier --write **/src/**/*.tsx",
		"relay": "lerna run relay --parallel --ignore @workshop/0* --ignore @workshop/1*",
		"seed": "pnpm workspace @workshop/server seed",
		"server": "pnpm workspace @workshop/server start",
		"server:serve": "pnpm workspace @workshop/server serve",
		"start": "concurrently --kill-others \"pnpm workspace @workshop/web start\" \"pnpm workspace @workshop/server start\"",
		"update-schema": "pnpm w apps/server/scripts/updateSchema.ts",
		"schema": "pnpm w apps/server/scripts/updateSchema.ts",
		"w": "pnpm b webpackx.ts",
		"web": "pnpm workspace @workshop/web start",
		"web-react-router": "pnpm workspace @workshop/web-react-router start",
		"web:rspack": "pnpm workspace @workshop/web-react-router rspack:dev",
		"next:dev": "pnpm workspace @workshop/next dev",
		"next:build": "pnpm workspace @workshop/next build",
		"nextapp:dev": "pnpm workspace @workshop/nextapp dev",
		"nextapp:build": "pnpm workspace @workshop/nextapp build"
	},
	"packageManager": "pnpm@9.7.0",
	"workspaces": {
		"packages": ["apps/*", "packages/*", "workshop/*", "solutions/*"]
	}
}
